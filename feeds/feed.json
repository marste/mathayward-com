---
layout: none
---
[
{% for post in site.posts %}
    {% if post.title != null and post.title != empty %}
    {% if forloop.index > 1 %},{%endif%}{
        "title": "{{post.title}} ",
        "content": "{{post.content | markdownify | strip_html | strip_newlines | replace: '"', '\"'}}",
        "link": "{{ site.url }}{{ post.url }}",
        "date": "{{ post.date }}",
        "excerpt": "{{ post.snippet }}",
        "seach_omit": "{{ post.search_omit }}"
    }
    {%endif%}
{% endfor %}

{% for page in site.pages %}
    {% if page.layout != 'none' and page.layout != 'none' and page.title != null and page.title != empty %}
        {% if forloop.index > 1 %},{%endif%}{
            "title": "{{page.title}} ",
            "content": "{{page.content | strip_html | strip_newlines | replace: '"', '\"'}}",
            "link": "{{ site.url }}{{ page.url | replace: 'index.html', '' }}",
            "date": "{{ page.date }}",
            "excerpt": "{{ page.description }}",
            "seach_omit": "{{ post.search_omit }}"
        }
    {%endif%}
{% endfor %}
]